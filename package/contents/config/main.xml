<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 https://www.kde.org/standards/kcfg/1.0/kcfg.xsd">

	<kcfgfile name="" />

	<group name="General">
		<entry name="useSystemColors" type="Bool">
			<default>true</default>
		</entry>

		<entry name="foregroundColor" type="String">
			<default>#E6E6E6</default>
		</entry>

		<entry name="backgroundColor" type="String">
			<default>#1E1E1E</default>
		</entry>

		<entry name="transparentBackground" type="Bool">
			<default>false</default>
		</entry>

		<entry name="backgroundOpacity" type="Double">
			<default>1.0</default>
		</entry>

		<entry name="fontPixelSize" type="Int">
			<default>10</default>
		</entry>

		<entry name="refreshIntervalMs" type="Int">
			<default>1000</default>
		</entry>
	</group>
</kcfg>
